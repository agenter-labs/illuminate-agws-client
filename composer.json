{
    "name": "agenter-labs/illuminate-agws-client",
    "description": "Http Client",
    "keywords": [
        "laravel",
        "http",
        "client"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "firebase/php-jwt": "^6.0",
        "illuminate/http": "^9.0",
        "illuminate/cache": "^9.15",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "laravel/lumen-framework": "^9.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
    "psr-4": {
            "AgenterLab\\AGWS\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\AGWS\\AGWSServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
